@model IEnumerable<MVCDemo.Models.ProjectModel>
@using MVCDemo.Models

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="font-family:Arial">
    <h2>Project List</h2>
    <ul>
        @foreach (ProjectModel project in @Model)
        {
            <li>
                @Html.ActionLink(project.Title, "Details","Employee",new { id = project.EmployeeId }, null);
            </li>
        }
    </ul>
</div>


